<!DOCTYPE html>
<html lang="zxx">


<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <title>TRANSHA STAYS | RESORT DETAILS</title>
    <link rel="shortcut icon" href="/img/transha/logo.jpeg" />
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Libre+Caslon+Display&amp;family=Outfit:wght@300;400&amp;display=swap">
    <link rel="stylesheet" href="/css/plugins.css" />
    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
</head>

<body>
    <!-- Preloader -->
    <div class="preloader-bg"></div>
    <div id="preloader">
        <div id="preloader-status">
            <div class="preloader-position loader"> <span></span> </div>
        </div>
    </div>
    <!-- Progress scroll totop -->
    <div class="progress-wrap cursor-pointer">
        <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
            <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
        </svg>
    </div>
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg">
        <div class="container">
            <!-- Logo -->
            <div class="logo-wrapper">
                <a class="logo" href="/"> <img src="/img/transha/logo.png" class="logo-img" alt=""> </a>
            </div>
            <!-- Button -->
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar"
                aria-controls="navbar" aria-expanded="false" aria-label="Toggle navigation"> <span
                    class="navbar-toggler-icon"><i class="ti-menu"></i></span> </button>
            <!-- Menu -->
            <div class="collapse navbar-collapse" id="navbar">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item "> <a class="nav-link  " href="/" role="button">Home </a>

                    </li>
                   
                 <li class="nav-item dropdown"> <a class="nav-link dropdown-toggle" href="#" role="button"
                            data-bs-toggle="dropdown" data-bs-auto-close="outside" aria-expanded="false">Resort
                            Accommodations <i class="ti-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                <% if (Array.isArray(resorts) && resorts.length > 0) { %>
                                    <% resorts.forEach(function(resort) { %>
                                        <li>
                                            <a href="/resortDetails/<%= encodeURIComponent(resort.slug || resort.name) %>" class="dropdown-item">
                                                <span><%= resort.name %></span>
                                            </a>
                                        </li>
                                    <% }); %>
                                <% } else { %>
                                    <li><span class="dropdown-item text-muted">No resorts available</span></li>
                                <% } %>
                            </ul>
                    </li>
                                        <li class="nav-item"><a class="nav-link" href="/near-by-locations">Places to Visit Nearby</a></li>
                    <li class="nav-item dropdown"> <a class="nav-link active dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" data-bs-auto-close="outside" aria-expanded="false">Services <i class="ti-angle-down"></i></a>
                  <ul class="dropdown-menu">
  <li><a href="/pg" class="dropdown-item active"><span>PG Stay</span></a></li>
  <li><a href="/spa-wellness" class="dropdown-item"><span>Ayurvedic Spa & Massage</span></a></li>
</ul>
                    </li>
                     <li class="nav-item"><a class="nav-link" href="/about">About</a></li>

            
                                    <li class="nav-item"><a class="nav-link" href="/blogs">Blogs</a></li>
                    <li class="nav-item"><a class="nav-link" href="/contact">Contact</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- PG Property Details Header -->
    <div class="room-details2 section-padding valign bg-img bg-fixed" data-overlay-dark="7"
        data-background="<%= pg.images && pg.images.length > 0 ? pg.images[0] : 'img/slider/6.jpg' %>">
        <div class="container resort-details-main">
            <div class="row">
                <div class="col-md-12">
                    <div class="wrap">
                        <figure>
                            <div class="owl-carousel owl-theme">
                                <% if (pg.images && pg.images.length > 0) { %>
                                  <% pg.images.forEach(function(img) { %>
                                    <div class="img"> <img src="<%= img %>" alt="<%= pg.name %>"> </div>
                                  <% }); %>
                                <% } else { %>
                                    <div class="img"> <img src="/img/rooms/19.jpg" alt="PG Room"> </div>
                                    <div class="img"> <img src="/img/rooms/20.jpg" alt="PG Room"> </div>
                                    <div class="img"> <img src="/img/rooms/21.jpg" alt="PG Room"> </div>
                                <% } %>
                            </div>
                        </figure>
                        <div class="caption">
                            <div class="title white mb-10"><%= pg.name %></div>
                            <% if (pg.rating && pg.rating > 0) { %>
                                <span>
                                    <% for (var i = 0; i < Math.floor(pg.rating); i++) { %>
                                        <i class="star-rating"></i>
                                    <% } %>
                                    <% for (var j = Math.floor(pg.rating); j < 5; j++) { %>
                                        <i class="star-rating grey"></i>
                                    <% } %>
                                    <span style="margin-left:10px;font-size:16px;color:#ffe48a;"><%= pg.rating %>/5</span>
                                </span>
                            <% } %>
                            
                            <ul class="list-inline info-list">
                                <li class="text-white"><strong>Address:</strong> <%= pg.address %><% if(pg.city){%>, <%= pg.city %><%}%> <% if(pg.state){%>, <%= pg.state %><%}%></li>
                                <li class="text-white"><strong>Contact:</strong> <%= pg.contactNumber || '+91 96058 32333' %></li>
                                <% if(pg.email){ %>
                                <li class="text-white"><strong>Email:</strong> <%= pg.email %></li>
                                <% } %>
                                <% if(pg.googleMapLink){ %>
                                <li >
                                    <a href="<%= pg.googleMapLink %>" target="_blank" style="color:#ffe48a;">
                                        <i class="ti-location-pin"></i> View on Google Maps
                                    </a>
                                </li>
                                <% } %>
                                <li class="text-white"><strong>Type:</strong> <%= pg.genderType %> PG</li>
                                <li class="text-white"><strong>Total Floors:</strong> 
                                    <%= pg.floors && Array.isArray(pg.floors) ? pg.floors.length : 0 %>
                                </li>
                                <li class="text-white"><strong>Total Rooms:</strong> 
                                    <% 
                                        let totalRooms = 0;
                                        if (pg.floors && Array.isArray(pg.floors)) {
                                            pg.floors.forEach(function(floor) {
                                                if (floor.allRooms && Array.isArray(floor.allRooms)) {
                                                    totalRooms += floor.allRooms.length;
                                                }
                                            });
                                        }
                                    %>
                                    <%= totalRooms %>
                                </li>
                                <li class="text-white"><strong class="text-white">Total Beds:</strong> 
                                    <%
                                        let totalBeds = 0;
                                        if (pg.floors && Array.isArray(pg.floors)) {
                                            pg.floors.forEach(function(floor) {
                                                if (floor.allRooms && Array.isArray(floor.allRooms)) {
                                                    floor.allRooms.forEach(function(room) {
                                                        if (room.allBeds && Array.isArray(room.allBeds)) {
                                                            totalBeds += room.allBeds.length;
                                                        }
                                                    });
                                                }
                                            });
                                        }
                                    %>
                                    <%= totalBeds %>
                                </li>
                            </ul>                     
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- PG Details Content -->
    <section class="room-details1 section-padding" data-scroll-index="1">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <% if (pg.description) { %>
                        <p class="mb-30"><%= pg.description %></p>
                    <% } else { %>
                        <p class="mb-30">
                            Enjoy a peaceful, comfortable, and budget-friendly living experience in our fully furnished PG
                            rooms. Every room is designed with modern essentials — clean bedding, ample ventilation, Wi-Fi,
                            and home-cooked meals to make your stay convenient and stress-free.
                        </p>
                    <% } %>

                    <div class="row">
                        <div class="col-md-6">
                            <h6>Check-in</h6>
                            <ul class="list-unstyled page-list mb-30">
                                <li>
                                    <div class="page-list-icon"><span class="ti-check"></span></div>
                                    <div class="page-list-text">
                                        <p>Check-in Time: <strong><%= pg.checkInTime || "12:00 PM" %></strong></p>
                                    </div>
                                </li>
                                <li>
                                    <div class="page-list-icon"><span class="ti-check"></span></div>
                                    <div class="page-list-text">
                                        <p>Prior booking confirmation required</p>
                                    </div>
                                </li>
                            </ul>
                        </div>
                        <div class="col-md-6">
                            <h6>Check-out</h6>
                            <ul class="list-unstyled page-list mb-30">
                                <li>
                                    <div class="page-list-icon"><span class="ti-check"></span></div>
                                    <div class="page-list-text">
                                        <p>Check-out Time: <strong><%= pg.checkOutTime || "11:00 AM" %></strong></p>
                                    </div>
                                </li>
                                <li>
                                    <div class="page-list-icon"><span class="ti-check"></span></div>
                                    <div class="page-list-text">
                                        <p>Refundable security deposit required</p>
                                    </div>
                                </li>
                            </ul>
                        </div>

                        <div class="col-md-12">
                            <h6>House Rules</h6>
                            <% if (pg.rules && Array.isArray(pg.rules) && pg.rules.length > 0) { %>
                                <ul class="list-unstyled rules-list mb-3">
                                    <% pg.rules.forEach(function(rule, ruleIdx) { %>
                                        <li>
                                            <span class="badge bg-warning text-dark me-2" style="font-size: 0.92em;">
                                                <i class="ti-star"></i>
                                            </span>
                                            <span><%= rule %></span>
                                        </li>
                                    <% }); %>
                                </ul>
                            <% } else { %>
                                <p>
                                    Maintain cleanliness and respect other tenants’ privacy. Loud noise and parties are not
                                    permitted. Visitors are allowed only during specified hours.
                                </p>
                            <% } %>
                        </div>
  
                    </div>

                    <!-- FLOORS, ROOMS, BEDS SECTION -->
                    <div class="mt-5">
                        <h5 style="font-weight:600;color:#263238;margin-bottom:18px;">
                            <i class="ti-layout-tab"></i> Floors & Room Details
                        </h5>
                        <% if (pg.floors && Array.isArray(pg.floors) && pg.floors.length > 0) { %>
                            <% pg.floors.forEach(function(floor, fIdx) { %>
                                <div class="card mb-3">
                                    <div class="card-header bg-light" style="font-weight:600;">
                                        <i class="ti-layout-menu-v"></i>
                                        <%= floor.name || ("Floor " + (fIdx+1)) %>
                                        <% if(floor.description){ %>
                                            <span style="font-weight:400; color:#7b8a99;">&nbsp;&ndash; <%= floor.description %></span>
                                        <% } %>
                                    </div>
                                    <div class="card-body">
                                        <%
                                            
                                            let theRooms = [];
                                            if (floor.allRooms && Array.isArray(floor.allRooms)) {
                                                theRooms = floor.allRooms;
                                            }
                                        %>
                                        <% if(theRooms.length > 0){ %>
                                            <div class="accordion" id="floor-accordion-<%= fIdx %>">
                                                <% theRooms.forEach(function(room, rIdx){ %>
                                                    <div class="accordion-item mb-2">
                                                        <h2 class="accordion-header" id="heading-<%= fIdx %>-<%= rIdx %>">
                                                            <button class="accordion-button collapsed flex-wrap text-start" type="button"
                                                                data-bs-toggle="collapse"
                                                                data-bs-target="#collapse-<%= fIdx %>-<%= rIdx %>"
                                                                aria-expanded="false"
                                                                aria-controls="collapse-<%= fIdx %>-<%= rIdx %>">
                                                                <span style="margin-right:8px;"><i class="flaticon-bed"></i></span>
                                                                <span class="fw-semibold">Room <%= room.roomNumber || room.number || (rIdx + 1) %></span>
                                                                <div class="w-100 d-block d-md-none" style="height:7px"></div>
                                                                <span class="d-flex flex-wrap ms-0 ms-md-2" style="gap:5px;">
                                                                    <% if(room.sharingType){ %>
                                                                        <span class="badge rounded-pill bg-primary"><%= room.sharingType %> Sharing</span>
                                                                    <% } %>
                                                                    <% if(typeof room.capacity !== 'undefined'){ %>
                                                                        <span class="badge rounded-pill bg-info">
                                                                            <%= room.capacity %> Bed<%= room.capacity>1?'s':'' %>
                                                                        </span>
                                                                    <% } %>
                                                                    <% if(typeof room.pricePerMonth !== 'undefined'){ %>
                                                                        <span class="badge rounded-pill bg-success">₹ <%= room.pricePerMonth %> / mo</span>
                                                                    <% } %>
                                                                    <% if(room.depositAmount != null && room.depositAmount > 0){ %>
                                                                        <span class="badge rounded-pill bg-danger">Deposit: ₹ <%= room.depositAmount %></span>
                                                                    <% } %>
                                                                    <% if(room.status){ %>
                                                                        <span class="badge rounded-pill bg-warning text-dark"><%= room.status %></span>
                                                                    <% } %>
                                                                    <% if(room.isAvailable === false){ %>
                                                                        <span class="badge rounded-pill bg-secondary">Not Available</span>
                                                                    <% } %>
                                                                </span>
                                                            </button>
                                                        </h2>
                                                        <div id="collapse-<%= fIdx %>-<%= rIdx %>" class="accordion-collapse collapse"
                                                             aria-labelledby="heading-<%= fIdx %>-<%= rIdx %>">
                                                            <div class="accordion-body">
                                                                <div class="mb-2">
                                                                    <% if(room.amenities && Array.isArray(room.amenities) && room.amenities.length > 0){ %>
                                                                        <div style="margin-bottom:8px;"><strong>Amenities:</strong>
                                                                            <% room.amenities.forEach(function(rAmenity){ %>
                                                                                <span class="badge text-dark me-1" ><%= rAmenity %></span>
                                                                            <% }) %>
                                                                        </div>
                                                                    <% } %>
                                                                    <% if(room.images && Array.isArray(room.images) && room.images.length > 0){ %>
                                                                        <div class="mb-2">
                                                                            <% room.images.forEach(function(img, imgIdx){ %>
                                                                                <img src="<%= img %>" alt="Room Image" style="width:60px;height:40px;object-fit:cover;border-radius:5px;margin-right:5px;">
                                                                            <% }) %>
                                                                        </div>
                                                                    <% } %>
                                                                </div>
                                                                <%
                                                                   
                                                                    let theBeds = [];
                                                                    if (room.allBeds && Array.isArray(room.allBeds)) {
                                                                        theBeds = room.allBeds;
                                                                    }
                                                                %>
                                                                <% if(theBeds.length > 0){ %>
                                                                    <div class="row g-3">
                                                                        <% theBeds.forEach(function(bed, bIdx){ %>
                                                                            <div class="col-12 col-md-6 col-lg-4">
                                                                                <div class="bed-card p-2 border rounded h-100" style="background:#f4f5fa;">
                                                                                    <div class="mb-2">
                                                                                        <span style="font-weight:600;">
                                                                                            <i class="flaticon-bed me-2"></i>Bed <%= bed.bedNumber || bed.number || (bIdx+1) %>
                                                                                        </span>
                                                                                        <% if(bed.type){ %>
                                                                                            <span class="badge bg-secondary ms-1"><%= bed.type %></span>
                                                                                        <% } %>
                                                                                        <% if(typeof bed.isOccupied !== 'undefined'){ %>
                                                                                            <% if(bed.isOccupied){ %>
                                                                                                <span class="badge bg-danger ms-1">Occupied</span>
                                                                                            <% } else { %>
                                                                                                <span class="badge bg-success ms-1">Available</span>
                                                                                            <% } %>
                                                                                        <% } %>
                                                                                    </div>
                                                                                    <% if(bed.amenities && Array.isArray(bed.amenities) && bed.amenities.length > 0){ %>
                                                                                        <ul class="list-unstyled mb-1" style="font-size:0.96em;">
                                                                                            <% bed.amenities.forEach(function(bamenity) { %>
                                                                                                <li>
                                                                                                    <span class="badge bg-warning text-dark me-1" style="font-size:0.86em;">
                                                                                                        <i class="ti-check-box"></i>
                                                                                                    </span>
                                                                                                    <%= bamenity %>
                                                                                                </li>
                                                                                            <% }) %>
                                                                                        </ul>
                                                                                    <% } %>
                                                                                    <% if(typeof bed.price !== 'undefined'){ %>
                                                                                        <div><strong>₹ <%= bed.price %> / month</strong></div>
                                                                                    <% } %>
                                                                                </div>
                                                                            </div>
                                                                        <% }) %>
                                                                    </div>
                                                                <% } else { %>
                                                                    <div class="text-muted">No beds information available.</div>
                                                                <% } %>
                                                            </div>
                                                        </div>
                                                        <div class="px-4 py-2 border-top small text-muted" style="background:#fafbfc;">
                                                            <span><strong>Room Number:</strong> <%= room.roomNumber || room.number || (rIdx+1) %></span>
                                                            <% if(room.sharingType){ %>
                                                                &nbsp;| <strong>Sharing:</strong> <%= room.sharingType %>
                                                            <% } %>
                                                            <% if(typeof room.capacity !== 'undefined'){ %>
                                                                &nbsp;| <strong>Capacity:</strong> <%= room.capacity %>
                                                            <% } %>
                                                            <% if(typeof room.pricePerMonth !== 'undefined'){ %>
                                                                &nbsp;| <strong>Price/Month:</strong> ₹ <%= room.pricePerMonth %>
                                                            <% } %>
                                                            <% if(room.depositAmount != null && room.depositAmount > 0){ %>
                                                                &nbsp;| <strong>Deposit:</strong> ₹ <%= room.depositAmount %>
                                                            <% } %>
                                                            <% if(room.status){ %>
                                                                &nbsp;| <strong>Status:</strong> <%= room.status %>
                                                            <% } %>
                                                            <% if(room.amenities && Array.isArray(room.amenities) && room.amenities.length > 0){ %>
                                                                &nbsp;| <strong>Amenities:</strong>
                                                                <% room.amenities.forEach(function(rAmenity, raidx){ %>
                                                                    <span class="badge text-dark ms-1" style="background-color: #c2e0ff;"><%= rAmenity %></span>
                                                                <% }) %>
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                <% }) %>
                                            </div>
                                        <% } else { %>
                                            <div class="text-muted">No rooms information found for this floor.</div>
                                        <% } %>
                                    </div>
                                </div>
                            <% }) %>
                        <% } else { %>
                            <div class="alert alert-secondary mt-4">No floors or detailed room information available for this property.</div>
                        <% } %>
                    </div>
                    <!-- END FLOORS, ROOMS, BEDS SECTION -->
                </div>

                <div class="col-md-3 offset-md-1">
                    <h6>Amenities</h6>
                    <% if(pg.amenities && Array.isArray(pg.amenities) && pg.amenities.length) { %>
                        <ul class="list-unstyled amenities-simple-list mb-3">
                            <% pg.amenities.slice(0, 12).forEach(function(amenity) { %>
                                <li style="margin-bottom:5px;">
                                    <span class="amenity-label"><%= amenity %></span>
                                </li>
                            <% }) %>
                            <% if (pg.amenities.length > 12) { %>
                                <li class="text-muted small">And <%= pg.amenities.length - 12 %> more...</li>
                            <% } %>
                        </ul>
                        <style>
                            .amenities-simple-list {
                                padding-left: 0;
                                margin-bottom: 0;
                            }
                            .amenities-simple-list li {
                                list-style-type: disc;
                                margin-left: 18px;
                                font-size: 1em;
                                color: #212529;
                            }
                            .amenity-label {
                                padding: 2px 0;
                                display: block;
                            }
                        </style>
                    <% } else { %>
                        <ul class="list-unstyled amenities-simple-list mb-3">
                            <li>Single / Sharing Options</li>
                            <li>Free Wi-Fi Access</li>
                            <li>Spacious 200 sq.ft Room</li>
                            <li>Home-Cooked Meals</li>
                            <li>Regular Laundry & Cleaning</li>
                            <li>Common Recreation Area</li>
                        </ul>
                        <style>
                            .amenities-simple-list {
                                padding-left: 0;
                                margin-bottom: 0;
                            }
                            .amenities-simple-list li {
                                list-style-type: disc;
                                margin-left: 18px;
                                font-size: 1em;
                                color: #212529;
                            }
                        </style>
                    <% } %>
               
                </div>
            </div>
        </div>
    </section>

    <!-- Booking Search -->

    <!-- Footer -->
    <footer class="footer">
        <div class="top">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 mb-30">
                        <div class="sub-title border-footer-light whte">Transha Stays</div>
                        <p style="color: white;">Transha Stays offers premium resorts, PG accommodations, and authentic
                            Noorani Ayurveda massage experiences. Relax, rejuvenate, and enjoy a comfortable stay with
                            us.</p>
                    </div>
                    <div class="col-md-4 offset-md-1">
                        <div class="item">
                            <h3>Get in touch</h3>
                            <p>Kerala, Thiruvananthapuram, India</p>

                        <p class="phone">+91 96058 32333</p>
                        <p class="mail">info@transhastays.com</p>
                        <div class="social mt-2"> 
                            <a href="https://www.instagram.com/transha_stays?utm_source=ig_web_button_share_sheet&igsh=ZDNlZDc0MzIxNw==" target="_blank"><i class="ti-instagram"></i></a>
                            <a href="#"><i class="ti-twitter"></i></a> 
                            <a href="#"><i class="ti-linkedin"></i></a> 
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="item">
                        <h3>Quick Links</h3>
                   <ul class="footer-explore-list list-unstyled">
                                <li><a href="/">Home</a></li>
                                <li><a href="/resorts">Resort Accommodations</a></li>
                                <li><a href="/near-by-locations">Places to Visit Nearby</a></li>
                                 <li><a href="/pg">PG Stays</a></li>
                                <li><a href="/spa-wellness">Nooranu Ayurvedic Massage</a></li>
                                <li><a href="/about">About</a></li>
                                         <li><a href="/blogs">Blogs</a></li>
                                <li><a href="/contact">Contact</a></li>
                            </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="bottom">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <p style="text-align: center;">© 2025 Transha Stays. All rights reserved.</p>
                </div>
           
            </div>
        </div>
    </div>
</footer>
    <div class="contact-container">
        <div class="contact-btn whatsapp-btn">
            <div class="contact-pulse"></div>
            <i class="fab fa-whatsapp"></i>
            <span class="contact-label">Chat on WhatsApp</span>
        </div>

        <div class="contact-btn phone-btn">
            <div class="contact-pulse phone-pulse"></div>
            <i class="fas fa-phone-alt"></i>
            <span class="contact-label">Call +91 96058 32333</span>
        </div>
    </div>
    <nav class="mobile__bottom__navbar">
        <!-- Home -->
        <div class="nav__item__container " data-page="home">
            <div class="nav__icon__wrapper">
                <svg class="nav__icon__svg" viewBox="0 0 24 24">
                    <path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path>
                    <polyline points="9 22 9 12 15 12 15 22"></polyline>
                </svg>
                <div class="active__indicator__dot"></div>
            </div>
            <span class="nav__label__text">Home</span>
        </div>

        <!-- Resort -->
        <div class="nav__item__container" data-page="resort">
